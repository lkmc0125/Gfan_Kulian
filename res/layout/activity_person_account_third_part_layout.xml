<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d3d3d3"
    android:orientation="vertical" >
    
    <RelativeLayout 
    	    android:layout_width="fill_parent"
    	    android:layout_height="48dip"
    	    android:layout_marginLeft="4dip"
    	    android:layout_marginRight="4dip"
    	    android:layout_marginTop="10dip"
    	    android:background="@drawable/person_account_normol_bg"
    	    android:orientation="horizontal"
    	    android:id="@+id/person_account_my_task_value_layout"
    	    >
    	    <ImageView
    	        android:contentDescription="@null"
				android:id="@+id/person_account_my_task_value_img1"
				android:layout_width="25dip"
				android:layout_height="25dip"
				android:layout_alignParentLeft="true"
				android:layout_centerInParent="true"
				android:layout_marginLeft="10dip"
				android:background="@drawable/app_icon"
				android:textColor="#00ffff" />
    	    <TextView 
    	        android:id="@+id/person_account_my_task_value_text"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:layout_gravity="left|center"
    	        android:layout_marginLeft="10dip"
    	        android:layout_centerVertical="true"
    	        android:text="@string/person_account_my_task_value"
    	        android:textSize="14sp"
    	        android:textColor="#000000"
    	        android:layout_toRightOf="@+id/person_account_my_task_value_img1"
    	        />
    	     <TextView 
    	        android:id="@+id/person_account_my_task_value_hint_text"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:layout_gravity="left|center"
    	        android:layout_marginRight="10dip"
    	        android:layout_centerVertical="true"
    	        android:layout_alignParentRight="true"
    	        android:text="@string/person_account_my_task_hint_value"
    	        android:textSize="14sp"
    	        android:textColor="#808080"
    	        />

    	    
    	</RelativeLayout>
     <RelativeLayout 
    	    android:layout_width="fill_parent"
    	    android:layout_height="48dip"
    	    android:layout_marginLeft="4dip"
    	    android:layout_marginRight="4dip"
    	    android:layout_marginTop="10dip"
    	    android:background="@drawable/input_box_bottom_shape_bg"
    	    android:orientation="horizontal"
    	    android:id="@+id/person_account_message_center_value_layout"
    	    >
        	<ImageView
    	        android:contentDescription="@null"
				android:id="@+id/person_account_message_center_value_img1"
				android:layout_width="25dip"
				android:layout_height="25dip"
				android:layout_alignParentLeft="true"
				android:layout_centerInParent="true"
				android:layout_marginLeft="10dip"
				android:background="@drawable/app_icon"
				android:textColor="#00ffff" />
    	    <TextView 
    	        android:id="@+id/person_account_message_center_value_text"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:layout_gravity="left|center"
    	        android:layout_marginLeft="10dip"
    	        android:layout_centerVertical="true"
    	        android:text="@string/person_account_message_center_value"
    	        android:layout_toRightOf="@+id/person_account_message_center_value_img1"
    	        android:textSize="14sp"
    	        android:textColor="#000000"
    	        />


    	   
    	</RelativeLayout>

    	<RelativeLayout 
    	    android:layout_width="fill_parent"
    	    android:layout_height="48dip"
    	    android:layout_marginLeft="4dip"
    	    android:layout_marginRight="4dip"
    	    android:layout_marginTop="1dip"
    	    android:background="#ffffff"
    	    android:orientation="horizontal"
    	    android:id="@+id/person_account_normol_question_layout"
    	    >
    	    <ImageView
    	        android:contentDescription="@null"
				android:id="@+id/person_account_normol_question_img1"
				android:layout_width="25dip"
				android:layout_height="25dip"
				android:layout_alignParentLeft="true"
				android:layout_centerInParent="true"
				android:layout_marginLeft="10dip"
				android:background="@drawable/app_icon"
				android:textColor="#00ffff" />
    	    <TextView 
    	        android:id="@+id/person_account_normol_question_text"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:layout_gravity="left|center"
    	        android:layout_marginLeft="10dip"
    	        android:layout_centerVertical="true"
    	        android:text="@string/person_account_normol_question_value"
    	        android:textSize="14sp"
    	        android:textColor="#000000"
    	        android:layout_toRightOf="@+id/person_account_normol_question_img1"
    	        />

    	    
    	    
    	</RelativeLayout>

    	<RelativeLayout 
    	    android:layout_width="fill_parent"
    	    android:layout_height="48dip"
    	    android:layout_marginLeft="4dip"
    	    android:layout_marginRight="4dip"
    	    android:layout_marginTop="1dip"
    	    android:background="@drawable/input_box_top_shape_bg"
    	    android:orientation="horizontal"
    	    android:id="@+id/person_account_feedback_value_layout"
    	    >
    	    <ImageView
    	        android:contentDescription="@null"
				android:id="@+id/person_account_feedback_value_img1"
				android:layout_width="25dip"
				android:layout_height="25dip"
				android:layout_alignParentLeft="true"
				android:layout_centerInParent="true"
				android:layout_marginLeft="10dip"
				android:background="@drawable/app_icon"
				android:textColor="#00ffff" />
    	    <TextView 
    	        android:id="@+id/person_account_feedback_value_text"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:layout_gravity="left|center"
    	        android:layout_marginLeft="10dip"
    	        android:layout_centerVertical="true"
    	        android:text="@string/person_account_feedback_value"
    	        android:textSize="14sp"
    	        android:textColor="#000000"
    	        android:layout_toRightOf="@+id/person_account_feedback_value_img1"
    	        />

    	    
    	</RelativeLayout>
        
    	<RelativeLayout 
    	    android:layout_width="fill_parent"
    	    android:layout_height="48dip"
    	    android:layout_marginLeft="4dip"
    	    android:layout_marginRight="4dip"
    	    android:layout_marginTop="10dip"
    	    android:background="@drawable/person_account_normol_bg"
    	    android:orientation="horizontal"
    	    android:id="@+id/person_account_logout_value_layout"
    	    >
    	    <ImageView
    	        android:contentDescription="@null"
				android:id="@+id/person_account_logout_value_img1"
				android:layout_width="25dip"
				android:layout_height="25dip"
				android:layout_alignParentLeft="true"
				android:layout_centerInParent="true"
				android:layout_marginLeft="10dip"
				android:background="@drawable/app_icon"
				android:textColor="#00ffff" />
    	    <TextView 
    	        android:id="@+id/person_account_logout_value_text"
    	        android:layout_width="wrap_content"
    	        android:layout_height="wrap_content"
    	        android:layout_gravity="left|center"
    	        android:layout_marginLeft="10dip"
    	        android:layout_centerVertical="true"
    	        android:text="@string/person_account_logout_value"
    	        android:textSize="14sp"
    	        android:textColor="#000000"
    	        android:layout_toRightOf="@+id/person_account_logout_value_img1"
    	        />

    	   
    	    
    	</RelativeLayout>

</LinearLayout>
